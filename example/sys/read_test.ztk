[sys]
name: limiter
type: limiter
max: 100
min -100

[sys]
name: P
type: amplifier
gain: 2.0

[sys]
name: I
type: integrator
gain: 3.0
fgt: 1.0

[sys]
name: D
type: differentiator
gain: 4.0
tc: 0.5

[sys]
name: PID
type: PID
pgain: 1.0
igain: 2.0
dgain: 3.0
tc: 4.0
fgt: 0.1

[sys]
name: QPD
type: QPD
pgain: 2.0
dgain: 4.0
eps: 1.0e-5

[sys]
name: FOL
type: FOL
tc: 2.0
gain: -1.0

[sys]
name: SOL
type: SOL
t1: 1.0
t2: 2.0
damp: 3.0
gain: 4.0

[sys]
name: PhaseCompensator
type: phasecomp
t1: 3.0
t2: 4.0
gain: -2.0

[sys]
name: adapt
type: adapt
tc: 2.0
base: 100

[sys]
name: linearsystem
type: lin
a: ( 2, 2 ) {
  0, 1,
 -2,-3,
}
b: 2 { 0, 1 }
c: 2 { 1, 0 }
d: 0

[sys]
name: lineartransferfunction
type: tf
num: 1 { 0 1 }
den: 2 { 1 3 2 }

[sys]
name: moving_average
type: maf
ff: 0.01

[sys]
name: filter
type: butterworth
cf: 10.0
dim: 5

[sys]
name: step_generator
type: step
amp: 2.0
delay: 0.5
period: 1.2

[sys]
name: sinusoid
type: sine
amp: 1.5
delay: 0.7
period: 1.3

[connect]
step_generator 0 P 0
P 0 FOL 0
FOL 0 PID 0
PID 0 filter 0
